<h1>Listing companies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Address</th>
      <th>Is partner</th>
      <th>Partnership type</th>
      <th>Web site</th>
      <th>Facebook account</th>
      <th>Twitter account</th>
      <th>Github account</th>
      <th>Google plus account</th>
      <th>Logo url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @companies.each do |company| %>
      <tr>
        <td><%= company.name %></td>
        <td><%= company.description %></td>
        <td><%= company.address %></td>
        <td><%= company.is_partner %></td>
        <td><%= company.partnership_type %></td>
        <td><%= company.web_site %></td>
        <td><%= company.facebook_account %></td>
        <td><%= company.twitter_account %></td>
        <td><%= company.github_account %></td>
        <td><%= company.google_plus_account %></td>
        <td><%= company.logo_url %></td>
        <td><%= link_to 'Show', company %></td>
        <td><%= link_to 'Edit', edit_company_path(company) %></td>
        <td><%= link_to 'Destroy', company, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company', new_company_path %>
